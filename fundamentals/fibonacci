package fundamentos;

import java.util.Scanner;

public class fibonacci {

	public static void main(String[] args) {
		Scanner sc = new Scanner (System.in);
		
		int n,i,a,b,c;
		System.out.print("Digite o n√∫mero de termos: ");
		n = sc.nextInt();
		
		a = 0;
		b = 1;
		i = 2;
		
		if (n == 1) {
			System.out.print(a);
		}
		else {
			System.out.print(a +" "+ b +" ");
		}
		
		while (i < n) {
			c = a + b;
			System.out.print(c + " ");
			a = b;
			b = c;
			i++;
		}
		
		sc.close();
	}

}
